{
  "name": "domain-functions",
  "version": "1.0.1-rc.0",
  "description": "Decouple your business logic from your controllers. With first-class type inference from end to end.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "deno test --allow-env --allow-net --unstable src/*.test.ts",
    "build": "deno bundle src/index.ts dist/index.min.js",
    "cli": "deno run --allow-env --allow-net --unstable src/cli.ts",
    "prepublishOnly": "npm run npm-build && cd npm/",
    "npm-build": "deno run -A scripts/build-npm.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeasonedSoftware/domain-functions.git"
  },
  "author": "Seasoned Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeasonedSoftware/domain-functions/issues"
  },
  "homepage": "https://github.com/SeasonedSoftware/domain-functions"
}
